{
  "name": "stylelint-config-wordpress",
  "version": "9.1.1",
  "description": "WordPress shareable config for stylelint",
  "keywords": [
    "code",
    "code style",
    "css",
    "lint",
    "linter",
    "linting",
    "stylelint",
    "stylelint-config",
    "stylelint-config-wordpress",
    "validate",
    "wordpress",
    "WordPress"
  ],
  "author": "Stylelint",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntwb/stylelint-config-wordpress.git"
  },
  "homepage": "https://github.com/ntwb/stylelint-config-wordpress",
  "bugs": "https://github.com/ntwb/stylelint-config-wordpress/issues",
  "main": "index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.js",
    "scss.js"
  ],
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "eslint": "^3.0.0",
    "eslint-config-stylelint": "^6.0.0",
    "eslint-plugin-ava": "^4.0.0",
    "jest": "^19.0.2",
    "npm-run-all": "^4.0.0",
    "npmpub": "^3.0.3",
    "remark-cli": "^3.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^2.0.0",
    "stylelint": "^7.10.1"
  },
  "peerDependencies": {
    "stylelint": "^7.10.1"
  },
  "scripts": {
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "release": "npmpub --verbose",
    "test": "jest",
    "watch": "jest --watch"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "plugins": [
      "ava"
    ],
    "extends": [
      "stylelint"
    ],
    "env": {
      "jest": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  },
  "dependencies": {
    "stylelint-scss": "^1.4.4"
  }
}
